{
  "connectionString": "${file:/mnt/secrets/rpe-send-letter/APP_INSIGHTS_CONNECTION_STRING}",
  "role": {
    "name": "rpe-send-letter-service"
  },
  "preview": {
    "sampling": {
      "overrides": [
        {
          "telemetryType": "request",
          "attributes": [
            {
              "key": "http.url",
              "value": "https?://[^/]+/health.*",
              "matchType": "regexp"
            }
          ],
          "percentage": 0
        },
        {
          "telemetryType": "request",
          "attributes": [
            {
              "key": "http.response.status_code",
              "value": "0",
              "matchType": "strict"
            }
          ],
          "percentage": 0
        },
        {
          "telemetryType": "dependency",
          "percentage": 0
        }
      ]
    }
  }
}
