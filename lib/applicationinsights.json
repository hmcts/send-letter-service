{

  "connectionString": "${file:/mnt/secrets/rpe-send-letter/APP_INSIGHTS_CONNECTION_STRING}",
  "role": {
    "name": "rpe-send-letter-service"
  },
  "instrumentation": {
    "logging": {
      "level": "WARN"
    }
  },
  "preview": {
    "sampling": {
      "overrides": [
        {
          "telemetryType": "request",
          "attributes": [
            {
              "key": "http.url",
              "value": "https?://[^/]+/health.*",
              "matchType": "regexp"
            }
          ],
          "percentage": 1
        },
        {
          "telemetryType": "dependency",
          "percentage": 0
        }
      ]
    }
  }
}
